<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cy.hys.mapper.ManagerMapper">

    <select id="loginManager" resultType="com.cy.hys.bean.Manager">
        select * from manager where username=#{manager.username} and password = #{manager.password}
    </select>
</mapper>